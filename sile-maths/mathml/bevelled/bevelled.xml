<!--
  From https://github.com/w3c/mathml/issues/29
  Slightly modified:
   - removed rowspacing="0pt" on mtable's
   - removed mathsize="142%" on mi's in mtr's
  Rationale: I don't think these are necessary for a test case.
-->
<mathml mode="display" counter="bevel-mfrac-mathml">
  <mfrac bevelled="true">
    <mn>1</mn>
    <mn>2</mn>
  </mfrac>
</mathml>
<mathml mode="display" counter="bevel-mfrac-mathml">
  <mfrac bevelled="true">
    <mrow>
      <mi>x</mi>
      <mo>+</mo>
      <mi>y</mi>
      <mo>+</mo>
      <mi>z</mi>
    </mrow>
    <mn>3</mn>
  </mfrac>
</mathml>
<mathml mode="display" counter="bevel-mfrac-mathml">
  <mfrac bevelled="true">
    <msup>
      <mi>u</mi>
      <mo>′</mo>
    </msup>
    <msqrt>
      <mrow>
        <msup>
          <mi>u</mi>
          <mn>2</mn>
        </msup>
        <mo>+</mo>
        <mn>1</mn>
      </mrow>
    </msqrt>
  </mfrac>
</mathml>
<mathml mode="display" counter="bevel-mfrac-mathml">
  <mfrac bevelled="true">
    <mi mathvariant="normal">g</mi>
    <msup>
      <mi>cm</mi>
      <mn>3</mn>
    </msup>
  </mfrac>
</mathml>
<mathml mode="display" counter="bevel-mfrac-mathml">
  <mfrac bevelled="true">
    <mrow>
      <msup>
        <mi>x</mi>
        <mn>2</mn>
      </msup>
      <mo>+</mo>
      <msup>
        <mi>y</mi>
        <mn>2</mn>
      </msup>
    </mrow>
    <mrow>
      <msup>
        <mi>x</mi>
        <mn>2</mn>
      </msup>
      <mo>-</mo>
      <msup>
        <mi>y</mi><mn>2</mn>
      </msup>
    </mrow>
  </mfrac>
</mathml>
<mathml mode="display" counter="bevel-mfrac-mathml">
  <mfrac bevelled="true">
    <mfrac>
      <mn>1</mn>
      <mn>2</mn>
    </mfrac>
    <mfrac>
      <mn>2</mn>
      <mn>3</mn>
    </mfrac>
  </mfrac>
</mathml>
<mathml mode="display" counter="bevel-mfrac-mathml">
  <mfrac bevelled="true">
    <mrow>
      <mo>(</mo>
      <mtable>
        <mtr>
          <mtd columnalign="center">
            <mi>a</mi>
          </mtd>
        </mtr>
        <mtr>
          <mtd columnalign="center">
            <mi>b</mi>
          </mtd>
        </mtr>
        <mtr>
          <mtd columnalign="center">
            <mi>c</mi>
          </mtd>
        </mtr>
      </mtable>
      <mo>)</mo>
    </mrow>
    <mrow>
      <mo>(</mo>
      <mtable>
        <mtr>
          <mtd columnalign="center">
            <mi>a</mi>
          </mtd>
        </mtr>
        <mtr>
          <mtd columnalign="center">
            <mi>b</mi>
          </mtd>
        </mtr>
        <mtr>
          <mtd columnalign="center">
            <mi>c</mi>
          </mtd>
        </mtr>
      </mtable>
      <mo>)</mo>
    </mrow>
  </mfrac>
</mathml>
