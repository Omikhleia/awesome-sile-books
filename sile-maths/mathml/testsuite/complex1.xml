                                <maction actiontype="toggle"> 
                                  <mo>&#x22EF;</mo> 
                                  <mfrac> 
                                    <mn>1</mn> 
                                    <mrow> 
                                      <msubsup> 
                                        <mn>2</mn> 
                                        <mrow/> 
                                        <mstyle scriptlevel="2"> 
