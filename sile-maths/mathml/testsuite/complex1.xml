                  <mo>+</mo> 
                  <mfrac> 
                    <mn>1</mn> 
                    <mrow> 
                      <msubsup> 
                        <mn>2</mn> 
                        <mrow/> 
                        <mstyle scriptlevel="2"> 
                          <mn>1</mn> 
                        </mstyle> 
