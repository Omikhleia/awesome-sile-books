                              <mrow> 
                                <mo>-</mo> 
                                <mn>8</mn> 
                                <mi>&#x03C0;</mi> 
                              </mrow> 
                            </mstyle> 
                          </msubsup> 
                          <mrow> 
                            <mn>1</mn> 
                            <mo>+</mo> 
                            <mo>&#x2026;</mo> 
                          </mrow> 
                        </mfrac> 
                      </mrow> 
                    </mfrac> 
                  </mrow> 
                </mfrac> 
              </mrow> 
            </mfrac> 
          </mstyle> 
        </mrow> 
      </mtd> 
    </mtr> 
    <mtr> 
      <mtd> 
        <mtext>Another Ramanujan identity</mtext> 
      </mtd> 
      <mtd> 
        <mstyle displaystyle="true"> 
          <mrow> 
            <munderover> 
              <mo>&#x2211;</mo> 
              <mrow> 
                <mi>k</mi> 
                <mo>=</mo> 
                <mn>1</mn> 
              </mrow> 
              <mi>&#x221E;</mi> 
            </munderover> 
            <mfrac> 
              <mn>1</mn> 
              <msubsup> 
                <mn>2</mn> 
                <mrow/> 
                <mrow> 
                  <mo stretchy="false">&#x230A;</mo> 
                  <mi>k</mi> 
                  <mo>&#x00B7;</mo> 
                  <mtext>&#x200B;</mtext> 
                  <mi>&#x03C6;</mi> 
                  <mo stretchy="false">&#x230B;</mo> 
                </mrow> 
              </msubsup> 
            </mfrac> 
            <mo>=</mo> 
            <mstyle scriptlevel="1"> 
              <mfrac> 
